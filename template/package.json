    "@react-navigation/material-top-tabs": "6.0.2",
    "@react-navigation/native": "6.0.2",
    "@react-navigation/stack": "6.0.7",
    "appcenter": "4.4.3",
    "appcenter-analytics": "4.4.3",
    "appcenter-crashes": "4.4.3",
    "bignumber.js": "9.0.1",
    "dva": "2.4.1",
    "dva-core": "2.0.4",
    "dva-immer": "1.0.0",
    "dva-loading": "3.0.22",
    "dva-no-router": "1.2.1",
    "i18n-js": "3.8.0",
    "moment": "2.29.1",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-native": "0.66.3",
    "react-native-camera": "4.1.0",

{
  "name": "Awesome",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "podInstall": "arch -x86_64 npx pod-install",
    "getSvg": "node ./src/assets/svgs/getSvg.js",
    "autoInstall": "yarn && jetify && npm run podInstall && npm run getSvg",
    "env-beta": "node ./scripts/env.js -e beta",
    "env-production": "node ./scripts/env.js -e production",
    "apk-beta": "yarn env-beta && cd android && ./gradlew assembleReleaseBeta",
    "apk-prod": "yarn env-production && cd android && ./gradlew assembleReleaseProd && cd .. && yarn env-beta",
    "codepush-ios-beta": "yarn env-beta && appcenter codepush release-react -a jerryli720/Jerry-Template-iOS -d Staging -t 1.0.0",
    "codepush-android-beta": "yarn env-beta && appcenter codepush release-react -a jerryli720/Jerry-Template-Android -d Staging -t 1.0.0 && yarn env-beta",
    "codepush-android-prod": "yarn env-production && appcenter codepush release-react -a jerryli720/XXX-Android -d Production -t ~1.7.0 && yarn env-beta"
  },
  "dependencies": {
    "@ant-design/react-native": "4.2.0",
    "@react-native-community/async-storage": "1.12.1",
    "@react-native-community/cameraroll": "4.0.4",
    "@react-native-community/segmented-control": "2.2.2",
    "@react-native-community/slider": "4.1.3",
    "@react-native-masked-view/masked-view": "0.2.6",
    "@react-native-picker/picker": "1.16.7",
